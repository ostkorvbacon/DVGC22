<?xml version="1.0" encoding="utf-8"?>


<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical"

    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#6200EE"
        android:orientation="vertical"
        android:id="@+id/laymain"
        >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#6200EE"
        android:orientation="horizontal" >


        <Button
            android:id="@+id/menu1"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:text="@string/menu"
            android:textColor="#ffffff"
            />

        <TextView
            android:layout_width="235dp"
            android:layout_height="match_parent"
            android:text="@string/Outline"
            android:textSize="20sp"
            android:paddingTop="10dp"


            android:textColor="#ffffff"

            />

        <Button
            style="?android:attr/buttonBarButtonStyle"
            android:id="@+id/menu2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:textColor="#ffffff"
            android:text="@string/settings"

            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#6200EE"
        android:orientation="horizontal" >


        <Button
            android:id="@+id/subid1"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:text="@string/sub1"
            android:textColor="#ffffff"
            android:paddingStart="30dp"
            android:paddingLeft="30dp"
            />


        <Button
            android:id="@+id/subid2"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:text="@string/sub2"
            android:textColor="#ffffff"
            android:paddingStart="30dp"
            android:paddingLeft="30dp"
            />

        <Button
            android:id="@+id/subid3"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:text="@string/sub3"
            android:textColor="#ffffff"
            android:paddingStart="30dp"
            android:paddingLeft="30dp"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:orientation="horizontal"

        >

        <TextView
            android:id="@+id/texttest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/test"
            android:textColor="#000000"
            android:textSize="20sp"
            />

    </LinearLayout>
</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#6200EE"
        android:orientation="vertical"
        android:id="@+id/laymenu1"
        android:visibility="invisible"
        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="horizontal" >


            <Button
                android:id="@+id/node1"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:text="@string/node1"
                android:textColor="#ffffff"
                />



            <Button

                android:id="@+id/subnode1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:textColor="#ffffff"
                android:text="@string/subnode1"

                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="horizontal" >


            <Button
                android:id="@+id/node2"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:text="@string/node2"
                android:textColor="#ffffff"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:orientation="horizontal"
            android:layout_gravity="end">



            <Button
                android:id="@+id/back1"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:text="@string/back"
                android:textColor="#ffffff"
                />
            <Button
                android:id="@+id/log"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="200dp"
                android:layout_marginStart="200dp"
                android:text="@string/log"
                android:textColor="#ffffff"
                />


        </LinearLayout>



    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#6200EE"
        android:orientation="vertical"
        android:id="@+id/laymenu2"
        android:visibility="invisible"
        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#6200EE"
            android:orientation="vertical" >


            <Button
                android:id="@+id/value1"
                style="?android:attr/buttonBarButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:text="@string/value1"
                android:textColor="#ffffff"
                />

            <Button
                android:id="@+id/value2"
                style="?android:attr/buttonBarButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:text="@string/value2"
                android:textColor="#ffffff"
                />

            <Button
                android:id="@+id/value3"
                style="?android:attr/buttonBarButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:text="@string/value3"
                android:textColor="#ffffff"

                />


        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#6200EE"
            android:orientation="vertical"
            android:layout_gravity="end">



            <Button
                android:id="@+id/back2"
                style="?android:attr/buttonBarButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="@string/back"
                android:textColor="#ffffff"
                android:onClick="backfunc"
                />


        </LinearLayout>



    </LinearLayout>

</LinearLayout>

